{
  "servers": [
    {
      "bind": "127.0.0.1:8080",
      "default_host": {
        "server_name": "default",
        "locations": [
          {
            "path": "/",
            "config": {
              "root": "html/root/",
              "index": true,
              "index_files": [
                "index.html"
              ]
            }
          },
          {
            "path": "([a-zA-Z0-9\\s_\\\\.\\-\\(\\):])+(.php)$",
            "extension": "php",
            "config": {
              "root": "html/root/",
              "target": "localhost:9000",
              "index_files": [
                "index.php"
              ]
            }
          },
          {
            "path": "/wordpress/$",
            "extension": "php",
            "config": {
              "root": "html/root/",
              "target": "localhost:9000",
              "index_files": [
                "index.php"
              ]
            }
          }
        ]
      },
      "hosts": [

      ]
    }
  ]
}