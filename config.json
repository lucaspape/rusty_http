{
  "servers": [
    {
      "bind": "127.0.0.1:8080",
      "default_host": {
        "server_name": "default",
        "locations": [
          {
              "path": "/",
              "root": "html/root/",
              "index_files": [
                  "index.html"
              ],
              "config": {
                  "index": true
              }
          },
          {
              "path": "([a-zA-Z0-9\\s_\\\\.\\-\\(\\):])+(.php)$",
              "root": "html/root/",
              "extension": "php",
              "index_files": [
                  "index.php"
              ],
              "config": {
                  "target": "localhost:9000"
              }
          }
        ]
      },
      "hosts": [

      ]
    }
  ]
}